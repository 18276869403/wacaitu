/**index.wxss**/

page {
  background: #f5f7fb;
  padding-bottom: 120rpx;
}

.top {
  height: 600rpx;
  width: 94%;
  background: #fff;
  padding-left: 3%;
  padding-right: 3%;
  padding-bottom: 3%;
}

.banner {
  width: 100%;
  text-align: center;
  background: #fff;
  height: 340rpx;
}

.swiper {
  height: 320rpx;
  margin-bottom: 20rpx;
  width: 695rpx;
  margin: auto;
}

.slide-image {
  width: 694rpx;
  height: 306rpx;
  margin-top:15rpx;
  border-radius: 8rpx;
  margin-bottom: 25rpx;
}

.tongzhi {
  width: 100%;
  height: 60rpx;
  background: rgba(245, 247, 251, 1);
  border-radius: 26rpx;
}

.tongzhi-img {
  width: 15%;
  text-align: center;
}

.laba-img {
  width: 40rpx;
  height: 40rpx;
  margin-top: 10rpx;
}

.tongzhi-text {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #4e4e4e;
  width: 85%;
}

/* 纵向滚动样式 */

.swiper_container {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(85, 85, 85, 1);
}

.swiper_item {
  font-size: 28rpx;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 60rpx;
  line-height: 60rpx;
  width: 100%;
  text-align: left;
}

.sort {
  width: 100%;
  margin-top: 20rpx;
}

.sortImg {
  width: 25%;
  text-align: center;
}

.sort-img {
  width: 110rpx;
  height: 110rpx;
}

.sort-bottom {
  width: 100%;
}

.sort-text {
  width: 25%;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.content {
  margin-left: 3%;
  margin-right: 3%;
  width: 94%;
}

/* 求购列表样式 */

.content-qiugou {
  width: 100%;
}

.qiugou-title {
  width: 96%;
  margin-left: 2%;
  margin-right: 2%;
  height: 50rpx;
  line-height: 50rpx;
  position: relative;
  margin-top: 40rpx;
}

.qiugou-left {
  position: absolute;
  left: 0rpx;
}

.qiugou-right {
  position: absolute;
  right: 0rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(161, 161, 161, 1);
  opacity: 0.98;
}

.shuxian-img {
  width: 6rpx;
  height: 32rpx;
  margin-top: 9rpx;
  margin-right: 10rpx;
}

.qiugou-right-text {
  margin-right: 10rpx;
}

.enter-img {
  width: 75rpx;
  height: 25rpx;
  margin-top: 12.5rpx;
}

.qiugou-content {
}

.qiugou-list {
  width: 100%;
  height: 235rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(4, 40, 100, 0.09);
  border-radius: 10rpx 15rpx 15rpx 15rpx;
  margin-top: 24rpx;
  margin-bottom: 8rpx;
}

.list-left {
  width: 35%;
  text-align: center;
  position: relative;
}

.list-right {
  width: 55%;
  margin-left: 5%;
  padding-right: 5%;
  position: relative;
}

.tuoyuan-img {
  width: 156rpx;
  height: 135rpx;
  position: absolute;
  left: 0rpx;
  bottom: 0rpx;
}

.list-left-text {
  text-align: center;
  width: 100%;
  height: 156rpx;
  line-height: 156rpx;
  border-right: 1rpx solid rgba(227, 227, 227, 1);
  margin-top: 33rpx;
  font-size: 70rpx;
  color: rgba(51, 51, 51, 1);
  background: linear-gradient(163deg, rgba(0, 225, 235, 1) 0%, rgba(249, 139, 89, 1) 0%, rgba(253, 102, 41, 1) 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.tips {
  font-size: 36rpx;
  font-weight: normal;
  color: rgba(253, 103, 41, 1);
}

.list-right-jinxingzhong {
  width: 99rpx;
  height: 105rpx;
  position: absolute;
  right: 0rpx;
  top: 0rpx;
}

.list-right-title {
  width: 97%;
  margin-right: 3%;
  margin-top: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  border-bottom: 2rpx solid #f5f7fb;
  padding-bottom: 7rpx;
}

.list-right-item {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
}

.right-item-img {
  width: 10%;
  text-align: left;
}

.bianhao-img {
  width: 31rpx;
  height: 30rpx;
  margin-top: 10rpx;
}

.right-item-text {
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(88, 88, 88, 1);
  width: 90%;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.guige-img {
  width: 25rpx;
  height: 25rpx;
  margin-top: 12.5rpx;
}

.yanse-img {
  width: 26rpx;
  height: 28rpx;
  margin-top: 11rpx;
}

.pingou-item {
  width: 426rpx;
  height: 246rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(4, 40, 100, 0.09);
  border-radius: 10rpx;
  position: relative;
  margin-right: 26rpx;
  margin-top: 25rpx;
}

/* 拼购列表样式 */

.content-pingou {
  width: 100%;
  /* margin-top: 20rpx; */
}

.pingou-item-left {
  width: 86%;
  margin-left: 3%;
  margin-right: 3%;
}

.pingou-left-title {
  font-size: 27rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  height: 54rpx;
  line-height: 54rpx;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.pingou-left-item {
  font-size: 25rpx;
  font-weight: 500;
  color: rgba(104, 104, 104, 1);
  height: 45rpx;
  line-height: 45rpx;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.pingou-item-right {
  position: absolute;
  right: 0rpx;
}

.pingou-img {
  width: 32rpx;
  height: 246rpx;
}

.gundong {
  width: 100%;
}

/* 尾货样式开始 */

.weihuo {
  width: 100%;
  height: 513rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 4rpx 10rpx 0rpx rgba(4, 40, 100, 0.09);
  border-radius: 10rpx;
  margin-top: 25rpx;
}

.weihuo-item {
  height: 460rpx;
  width: 271rpx;
  border: 2rpx solid rgba(227, 227, 227, 1);
  border-radius: 10rpx;
  margin-left: 20rpx;
  margin-top: 25rpx;
}

.weihuoImg {
}

.weihuo-img {
  width: 271rpx;
  height: 214rpx;
  border-radius: 10rpx;
}

.weihuo-text {
  margin-left: 3%;
  margin-right: 3%;
  width: 94%;
}

/* 最新咨询样式开Ï始 */

.zixun {
  width: 100%;
}

.zixun-item {
  width: 96%;
  padding: 2%;
  height: 253rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 0rpx 7rpx 0rpx rgba(0, 41, 110, 0.15);
  border-radius: 6rpx;
  margin-top: 25rpx;
}

.left-date-text {
  margin-left: 10rpx;
  font-size: 22rpx;
  font-weight: 500;
  color: rgba(163, 163, 163, 1);
}

.shijian-img {
  width: 23rpx;
  height: 23rpx;
  margin-top: 13.5rpx;
}

.zixun-item-left {
  width: 70%;
  text-align: left;
  position: relative;
  height: 253rpx;
}

.zixun-item-right {
  width: 30%;
  text-align: right;
}

.zixun-img {
  width: 180rpx;
  height: 211rpx;
  border-radius: 10rpx;
  margin-top: 21rpx;
}

.zixun-left-title {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-top: 21rpx;
}

.zixun-left-content {
  font-size: 22rpx;
  font-weight: 400;
  color: rgba(80, 80, 80, 1);
  margin-top: 20rpx;
}

.zixun-left-date {
  position: absolute;
  bottom: 10rpx;
  height: 50rpx;
  line-height: 50rpx;
}

/* 弹窗 */

.commodity_screen1 {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.34;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
  text-align: center;
}

.commodity_attr_box1 {
  border-radius: 10px;
  position: fixed;
  z-index: 20000;
  width: 100%;
  height: 100%;
  text-align: center;
  justify-content: center;
  top: 30%;
}

.tanchuang {
  width: 536rpx;
  height: 389rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 20rpx;
  margin: auto;
  position: relative;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.tanchuang-item {
  width: 100%;
  text-align: center;
  position: absolute;
  top: -40rpx;
}

.tanchuang-img {
  width: 308rpx;
  height: 217rpx;
}

.tanchuang-text {
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-top: 10rpx;
}

.tanchuang-bottom {
  position: absolute;
  bottom: 0rpx;
  height: 70rpx;
  line-height: 70rpx;
  width: 100%;
  border-top: 1rpx solid #f1f1f1;
}

.tanchuang-bottom-left {
  font-size: 34rpx;
  font-weight: bold;
  color: rgba(171, 171, 171, 1);
  width: 50%;
  text-align: center;
  border-right: 1rpx solid #f1f1f1;
}

.tanchuang-bottom-right {
  width: 50%;
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  color: rgba(254, 107, 43, 1);
}

.bottom {
  position: fixed;
  bottom: 0rpx;
  right: -2rpx;
}

.xuanfu-img {
  width: 164rpx;
  height: 164rpx;
}
