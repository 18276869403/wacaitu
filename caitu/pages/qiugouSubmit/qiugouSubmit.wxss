page {
  background: #f5f7fb;
}

.content {
  width: 100%;
  margin-top: 30rpx;
}

.content-item {
  width: 90%;
  margin-left: 3%;
  margin-right: 3%;
  padding-left: 2%;
  padding-right: 2%;
  height: 83rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx 2rpx 9rpx 0rpx rgba(4, 40, 100, 0.06);
  border-radius: 15rpx;
  line-height: 83rpx;
  margin-bottom: 20rpx;
}

.enter-img {
  width: 13rpx;
  height: 23rpx;
}
.fuhao-img{
  width: 14rpx;
  height: 24rpx;
}

.content-item-left {
  width: 25%;
  text-align: left;
  font-size: 34rpx;
  color: rgba(51, 51, 51, 1);
}

.content-item-content {
  width: 70%;
  text-align: right;
  font-size: 32rpx;
  color: rgba(140, 140, 140, 1);
}

.content-item-right {
  width: 5%;
  text-align: right;
}

.content-item-content1 {
  width: 75%;
  text-align: right;
  font-size: 32rpx;
  color: rgba(140, 140, 140, 1);
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
}

.bottom {
  width: 100%;
  text-align: center;
  margin-top: 240rpx;
  margin-bottom: 115rpx;
}

.fabu-img {
  width: 570rpx;
  height: 78rpx;
}

.tips {
  width: 94%;
  margin-left: 3%;
  margin-right: 3%;
  text-align: left;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(246, 74, 36, 1);
  margin-top: 35rpx;
}
.shurukuang {
  width: 100%;
  height: 83rpx;
  line-height: 83rpx;
  font-size: 32rpx;
  color: rgba(140, 140, 140, 1);
}

.placeholder {
  height: 83rpx;
  line-height: 83rpx;
  font-size: 32rpx;
  color: rgba(140, 140, 140, 1);
}
