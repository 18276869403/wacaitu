/* pages/joinPingou/joinPingou.wxss */

.dingbian {
  border-top: 21rpx solid #f5f7fb;
}

.content {
  margin-top: 33rpx;
}

.content-list {
  width: 90%;
  margin-left: 3%;
  margin-right: 3%;
  height: 76rpx;
  border: 2rpx solid rgba(221, 223, 225, 1);
  border-radius: 10rpx;
  padding-left: 2%;
  padding-right: 2%;
  line-height: 76rpx;
  margin-bottom: 27rpx;
}

.content-list-left {
  width: 30%;
  text-align: left;
  font-size: 32rpx;
  font-weight: 500;
  color: #333;
}

.content-list-right {
  width: 70%;
  text-align: right;
  font-size: 30rpx;
  color: #333;
}

.shurukuang {
  width: 100%;
  height: 76rpx;
  line-height: 76rpx;
}

.placeholder {
  color: #8d8d8d;
}

.picker {
  color: #8d8d8d;
}

.content-list-right-area {
  width: 67%;
  text-align: right;
  font-size: 30rpx;
  color: #333;
}

.enter {
  width: 3%;
  text-align: right;
}

.enter-img {
  width: 9rpx;
  height: 19rpx;
  margin: auto;
}

.guding{
  position: fixed;
  bottom: 100rpx;
}